class Pattern3 
{
	public static void main(String[] args) 
	{
		int num=1;

		for (int r=1;r<=5 ;r++)
		{
			
			for (int c=1;c<=5 ;c++ )
			{
				if (r==1)
				{
					System.out.print(num++);
				}
				else if (r==2)
				{
					num=3;
					System.out.print(num++);
				}
				else if (r==3)
				{
					num=3;
					System.out.print(num++);
				}
				else if (r==4)
				{
					num=4;
					System.out.print(num++);
				}
				else if (r==5)
				{
					num=5;
					System.out.print(num++);
				}

			}
		System.out.println("");
		}
	}
}
