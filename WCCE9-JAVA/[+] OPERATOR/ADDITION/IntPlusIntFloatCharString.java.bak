//QNO1=Plus Oprator Is Used To Perform Addition
class IntPlusIntFloatCharString
{
	public static void main(String[] args) 
	{
		System.out.println(2+2);//int+int=int
		System.out.println(2+'A');//int+char=int
		System.out.println(2+10.85);//int+float=float
		System.out.println(2+"A");//int+string=string

		//System.out.println(2+true);//int+boolean=CTE
	}
}
