//QNO9:-Write A Program To Print First Five Lowercase Charcters Into Integer Format

class PrintLowerCaseCharToInt 
{
	public static void main(String[] args) 
	{
		int num='a';

		while(num<='e')
		{
			System.out.println(num);
			num++;
		}
	
	}
}
