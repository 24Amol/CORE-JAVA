//QNO10:-Write A Program To Calculate  Simple Interest
class SimpleInterest
{
	public static void main(String[] args) 
	{
		float P=10000, R=5, T=5;
		float SI=(P*R*T)/100;
		System.out.println("Simple Interest = " + SI);
	}
}
