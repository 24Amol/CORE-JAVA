//QNO3=Design A Method for Addition Of Three Number And Return Value Back Print The Result

class AdditionThreeNumReturnValue
{
	public static int add(int a, int b, int c)
	{
		int res=a+b+c;
		return res;
	}
	public static void main(String[] args) 
	{
		System.out.println(add(5,5,5));
	}
}
