//QNO=1:Write A Program To calculate The Area Of The Circle
class AreaCircle 
{
	public static void main(String[] args) 
	{
		int radius;
		double pi=3.142,area;
		radius=5;
		System.out.println("Radius:- " +radius);//Concat The String + Integer Value
		area=pi*radius*radius;//Formula For Calculating The Area Of The Circle
		System.out.println("Area Of Circle Is:- " + area);
	}
}

/*******************OUTPUT*******************/
//The Area Of Circle Is:-78.55				*
