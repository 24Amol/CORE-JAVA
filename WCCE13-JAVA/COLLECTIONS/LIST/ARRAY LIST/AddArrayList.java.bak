import java.util.ArrayList;

class AddArrayList 
{
	public static void main(String[] args) 
	{
		ArrayList oA=new ArrayList();
		
		//System.out.println(oA);

		//We can add Element in ArrayList Using add() Method
		oA.add(4);
		oA.add("Apple");
		oA.add('A');
		oA.add(11.11);

		ArrayList oA1=new ArrayList();
		
		//System.out.println(oA);

		//We can add Element in ArrayList Using add() Method
		oA1.add(4);
		oA1.add("Apple");
		oA1.add('A');
		oA1.add(11.11);

		oA1.remove(oA);

		System.out.println(oA1);


		System.out.println(oA);

	}
}
