//QNO17=Design A Method To Print The Product Of Three Numbers
class MethodProductThreeNum 
{

	public static void mul(int i,int j,int k)
	{
		int res=i*j*k;
		System.out.println(res);
	}
	public static void main(String[] args) 
	{
		mul(5,3,2);
	}
}
