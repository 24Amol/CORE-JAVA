//QNO19=Design A Method To Print The Subtraction Of Two Numbers
class MethodSubtractionTwoNum
{

	public static void sub(int i, int j)
	{	
		int res=i-j;
		System.out.println(res);
	}
	public static void main(String[] args) 
	{
		sub(5,3);
	}
}
