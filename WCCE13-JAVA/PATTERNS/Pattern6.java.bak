class Pattern6 
{
	public static void main(String[] args) 
	{
		for (int r=1;r<=5 ;r++ )
		{
			int num=1;

			for (int c=1;c<=5 ;c++ )
			{
				if (r%2==0)
				{
					System.out.print(" "+"*");
					if (c%2==0)
					{
						System.out.print(" "+num);
					}
					else
					{
						System.out.print(" "+"*");
					}
				}
				else
				{
					System.out.print(num);

					if (c%2==0)
					{
						System.out.print(" "+"*");
					}
					else
					{
						System.out.print(num);
					}

				}
			}
		System.out.println();
		}
	}
}
