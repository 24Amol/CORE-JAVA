class Pattern10 
{
	public static void main(String[] args) 
	{
		for (int r=1;r<=5;r++)
		{
			int num=1;
					
			for (int c=1;c<=5;c++)
			{
				if (c==r || c+r==6)
				{
					System.out.print(" "+"*");
					
				}
				else
				{
					System.out.print(" "+"#");
				}
			}
		System.out.println();
		}
	}
}
