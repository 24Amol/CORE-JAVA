//QNO1=Can We declare a local variable inside a static block

class StaticMethodAddition
{
	static public int add(int a,int b)
	{
		int res=a+b;
		return res;
	}
	public static void main(String[] args) 
	{
		System.out.println(add(3,5));
	}
}
